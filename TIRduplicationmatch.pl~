#!/usr/bin/perl -w
#Jenny McCarthy
#Seeing how many base pairs match in a TIR of a transposase
use strict;
my $sequence = ''; #ORF sequence
my $substrseq = substr($sequence, 8, 100); #change every time you want more base pairs
#print the first TIR
print "TIR 1 $substrseq \n";

#get the reverse compliment of the second TIR

my $length = 100;
my $TIRend = substr($sequence, -108, $length); #change every time you want to go up higher
#reverse compliment of TIR end
my $revcom = reverse $TIRend;

$revcom =~ tr/ACGTacgt/TGCAtgca/;
print "TIR 2 $revcom\n";

exit;

#open (FILE, $ARGV[0])

